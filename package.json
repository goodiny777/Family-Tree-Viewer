    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/d3-hierarchy": "^3.1.7",
    "@types/d3-shape": "^3.1.7",
    "@types/react": "^18.3.18",
